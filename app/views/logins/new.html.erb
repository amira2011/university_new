<div class="container">
<div class="col ">
 <h3>   Log in </h3>
</div>

<%= form_for(:logins) do |f| %>

   



    <div class="form-group">
       <%= f.label :email %>
       <%= f.email_field :email, class: 'form-control' , id: 'studentlemail', placeholder: 'Enter Email'  , autofocus: true  %>  
       </div>

       
       <div class="form-group">
       <%= f.label :password %>
       <%= f.password_field :password, class: 'form-control' , id: 'studentlpass', placeholder: 'Enter Password' %>  
       
    </div>

 

  
    <div class="p-2">
    <%= f.submit("Log in",  class: "btn btn-primary") %> 
    </div>
    <% end %>
    </div>